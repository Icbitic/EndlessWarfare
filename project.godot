[application]

config/name="Endless Warfare"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name=".endlesswarfare"
boot_splash/image="res://bootsplash.png"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

Settings="*res://settings.gd"
Logger="*res://logger.gd"
Console="*res://addons/quentincaffeino/console/src/Console.tscn"
CellController="*res://classes/cell_controller.gd"

[display]

window/dpi/allow_hidpi=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/quentincaffeino/console/plugin.cfg" )

